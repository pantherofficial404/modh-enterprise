{
  "name": "modh-enterprise",
  "version": "1.0.0",
  "description": "A shopping store opening soon",
  "scripts": {
    "start": "npm run start:backend",
    "install": "npm run install:backend",
    "install:backend": "cd backend && npm install && npm install --only=dev && cd -",
    "start:backend": "cd backend && npm start && cd -",
    "lint:fix": "npm run lint:backend:fix",
    "lint:backend:fix": "cd backend && npm run lint -- --fix && cd -",
    "lint": "npm run lint:backend",
    "lint:backend": "cd backend && npm run lint && cd -"
  },
  "engines": {
    "node": "10.x",
    "npm": "6.x",
    "yarn": "1.x"
  },
  "cacheDirectories": [
    "backend/node_modules"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pantherofficial404/modh-enterprise.git"
  },
  "keywords": [],
  "author": "Vikram Modh",
